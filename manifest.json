{
  "manifest_version": 2,
  "name": "p5 sketch",
  "version": "0.1",
  "permissions": [
    "activeTab"
  ],
  "browser_action": {
      "default_title": "Set this page's color.",
      "default_icon": "icon.png",
      "default_popup": "popup.html"
  },
  "manifest_version": 2,
  "content_scripts": [{
    "matches": [
      "<all_urls>"
    ],
    "js": ["p5.js", "sketch.js"]

  }]
}
